<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.example.zwx.activity"
    android:installLocation="preferExternal" >

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="visitRecord.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="visitRecord.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="visitRecord.activity.PreviewActivity"
            android:label="@string/title_activity_preview" >
        </activity>
        <activity
            android:name="visitRecord.activity.AddRecordActivity"
            android:label="@string/title_activity_add_record" >
        </activity>
        <activity
            android:name="visitRecord.activity.ReleaseVisitActivity"
            android:label="@string/title_activity_release_visit" >
        </activity>
        <activity
            android:name="visitRecord.activity.LastRecordActivity"
            android:label="@string/title_activity_last_record" >
        </activity>
        <activity
            android:name="visitRecord.activity.LastNeedActivity"
            android:label="@string/title_activity_last_need" >
        </activity>
        <activity
            android:name="visitRecord.activity.ListDisplay"
            android:label="@string/title_activity_last_need" >
        </activity>
        <activity
            android:name="visitRecord.activity.AddMemberActivity"
            android:label="@string/title_activity_add_member" >
        </activity>
        <activity
            android:name="visitRecord.activity.VisitDetailActivity"
            android:label="@string/title_activity_visit_detail" >
        </activity>
        <activity
            android:name="visitRecord.activity.PersonInfoActivity"
            android:label="@string/title_activity_person_info" >
        </activity>
        <activity
            android:name="visitRecord.activity.VisitArrangeActivity"
            android:label="@string/title_activity_visit_arrange" >
        </activity>
        <activity
            android:name="visitRecord.activity.PreviewPersonInfoActivity"
            android:label="@string/title_activity_preview_person_info" >
        </activity>
        <activity
            android:name="visitRecord.activity.MyRecordActivity"
            android:label="@string/title_activity_my_record" >
        </activity>
        <activity
            android:name="visitRecord.activity.DeliveryDemandActivity"
            android:label="@string/title_activity_delivery_demand" >
        </activity>
        <activity
            android:name="visitRecord.activity.VisitDetailEditActivity"
            android:label="@string/title_activity_visit_detail_edit"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="orientation|keyboardHidden">
        </activity>
        <activity
            android:name="visitRecord.activity.LogsActivity"
            android:label="@string/title_activity_logs" >
        </activity>
    </application>

</manifest>
